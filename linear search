import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int arr[]=new int[n];
        int pos=-1;
        for(int i=0; i < n; i++){
            arr[i]=sc.nextInt();
        }
        int key=sc.nextInt();
        for(int i=0; i < n; i++){
            if(arr[i]==key){
                pos = i;
                break;
            }
        }
    if(pos != -1){
        System.out.println(pos);
    }
    else{
        System.out.println("-1");
    }
    }
}
