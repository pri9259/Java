import java.util.Scanner;

public class SmallestElement {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int N = sc.nextInt();   
        int smallest;

        int[] arr = new int[N];

        
        arr[0] = sc.nextInt();
        smallest = arr[0];

        for (int i = 1; i < N; i++) {
            arr[i] = sc.nextInt();
            if (arr[i] < smallest) {
                smallest = arr[i];
            }
        }

        
        System.out.println(smallest);

        sc.close();
    }
}
