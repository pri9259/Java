import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int M=s.nextInt();
        if(M>=90){
        System.out.println("A");
    }
          else if(M>=75){
        System.out.println("B");
    }
          else if(M>=60){
        System.out.println("C");
          }
        else
            System.out.println("Fail");
    }
}
