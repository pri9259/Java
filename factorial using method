import java.io.*;
import java.util.*;

public class Solution {
    public int factorial(int n){
    int factorial = 1;        
        for (int i = 1; i <= n; i++) {           
            factorial = factorial * i; 
           
        }
         return factorial;
             
    }
    public static void main(String[] args) {
               
        Solution ob=new Solution();
        Scanner s=new Scanner(System.in);
         int n=s.nextInt();
       int x=ob.factorial(n);
        System.out.println(x);       
    }
}
