import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String S=s.nextLine();
        StringBuilder sb=new StringBuilder(S);
        System.out.println("length:"+sb.length());
         System.out.println("capacity:"+sb.capacity());
                    
    }
}
