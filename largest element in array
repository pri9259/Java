import java.util.Scanner;

public class LargestElement {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int N = sc.nextInt();   
        int largest;

        int[] arr = new int[N];

        
        arr[0] = sc.nextInt();
        largest = arr[0];

        for (int i = 1; i < N; i++) {
            arr[i] = sc.nextInt();
            if (arr[i] > largest) {
                largest = arr[i];
            }
        }

        
        System.out.println(largest);

        sc.close();
    }
}
