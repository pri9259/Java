import java.io.*;
import java.util.*;


public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
            int n=s.nextInt();
            int a=0;
            while(n>0){
                a=a*10+n%10;
                n=n/10;
            }
            System.out.println(a);
        }
    }
        
