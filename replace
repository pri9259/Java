import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner s=new Scanner(System.in);
        String S=s.nextLine();
        String T=s.nextLine();
        int start=s.nextInt();
        int end=s.nextInt();
        StringBuilder sb=new StringBuilder(S);
        sb.replace(start,end,T);
        System.out.println(sb);
    }
}
