import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner (System.in);
        int n=s.nextInt();
        int a=0;
        for(int i=1;i<=n;i++){
            if(n%i==0){
                a++;
                
            }
        }
        System.out.println(a);
    }
}
