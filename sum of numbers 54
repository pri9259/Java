import java.util.*;
class SumCalculator {
    int sum(int a, int b) {
        return a + b;
    }
    int sum(int a, int b, int c) {
        return a + b + c;
    }
    double sum(double a, double b) {
        return a + b;
    }
}
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        SumCalculator calc = new SumCalculator();
        ArrayList<String> inputs = new ArrayList<>();
        while (sc.hasNext()) {
            inputs.add(sc.next());
        }
        if (inputs.size() == 2) {
            if (inputs.get(0).contains(".") || inputs.get(1).contains(".")) {
                double a = Double.parseDouble(inputs.get(0));
                double b = Double.parseDouble(inputs.get(1));
                System.out.println(calc.sum(a, b));
            } else {
                int a = Integer.parseInt(inputs.get(0));
             int b = Integer.parseInt(inputs.get(1));
            int c = Integer.parseInt(inputs.get(2));
            System.out.println(calc.sum(a, b, c));
        }

        sc.close();
    }
}
