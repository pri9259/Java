import java.io.*;
import java.util.*;

public class Solution {
    public int reverse (int n){
        int a=0;
        while(n>0){
            a=a*10+n%10;
            n=n/10;
            
        }
        return a;
    }

    public static void main(String[] args) {
        Solution ob=new Solution();
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int x=ob.reverse(n);
        System.out.println(x);     
    }
}
